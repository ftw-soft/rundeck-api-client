{
    "name": "ftw-soft/rundeck-api-client",
    "description": "Rundeck's API client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Telesh",
            "email": "andrey.telesh.y@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "psr/http-message": "^1.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.4",
        "guzzlehttp/guzzle": "^7.0",
        "http-interop/http-factory-guzzle": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "FtwSoft\\Rundeck\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FtwSoft\\Rundeck\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "guzzlehttp/guzzle" : "Required ^7.0 version to implement the standard package Http client"
    }
}
